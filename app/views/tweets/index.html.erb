<div class="container">
  <div class="row">
      <div class="wrapper col-md-8 col-md-offset-2 col-sm-10">
 <h1>ツイート一覧</h1><%= link_to "ツイートする", new_tweet_path, class: 'btn btn-info' %>
 
      </div>
  </div>
</div>

<div class="container">
  <div class="row">
      <div class="wrapper col-md-8 col-md-offset-2 col-sm-10">
      <% @tweets.each do |tweet| %>
      <div class="row">
        <ol class="breadcrumb"><p><%= tweet.content %></p>
        <%= link_to "編集", edit_tweet_path(tweet.id), class: 'btn btn-default' %>
        <%= link_to "削除", tweet_path(tweet.id), method: :delete, data: { confirm: '本当に削除していいですか？'}, class: 'btn btn-default btn-sm btn btn-danger' %>
      </div></ol>
      <% end %>
    </div>
  </div>
</div>